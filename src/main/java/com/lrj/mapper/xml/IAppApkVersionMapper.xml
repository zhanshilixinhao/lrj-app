<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd"
                >
<mapper namespace="com.lrj.mapper.IAppApkVersionMapper">
    <select id="getApkVersion" resultType="com.lrj.Vo.AppApkVersionVo">
        SELECT apkId AS apkId,
               apkMsg AS apkMsg,
               upTime AS upTime,
               version AS version
        FROM apk
    </select>

</mapper>